{
  "name": "x-follow-list-hide-mutuals",
  "displayName": "X Follow List Hide Mutuals",
  "version": "0.0.1",
  "description": "Toggle mutual followers in your Following list on X.com.",
  "author": "HigaTsubasa (info@cofus.work)",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "package-chrome": "pnpm plasmo build --zip",
    "package-firefox": "pnpm plasmo build --target=firefox-mv2 --zip"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*.png"
        ],
        "matches": [
          "*://x.com/*"
        ]
      }
    ],
    "host_permissions": [
      "*://x.com/*"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "info@cofus.work"
      }
    }
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
